{
  "name": "multibyte",
  "version": "1.0.2",
  "description": "Unicode-aware string functions",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensnyder/multibyte.git"
  },
  "keywords": [
    "multibyte",
    "unicode",
    "utf8",
    "utf-8",
    "utf-16",
    "utf16",
    "multibyte safe",
    "multibyte-safe",
    "unicode aware",
    "unicode-aware",
    "emoji",
    "emoji-safe"
  ],
  "scripts": {
    "test": "vitest run",
    "test-watch": "vitest",
    "clean": "rimraf dist/ && mkdir dist && touch dist/.gitkeep && mkdir dist/esm && mkdir dist/cjs",
    "build-esm": "npx -p typescript tsc --project tsconfig.esm.json",
    "build-cjs": "npx -p typescript tsc --project tsconfig.cjs.json",
    "build": "npm run clean && npm run build-esm && npm run build-cjs",
    "coverage": "vitest run --coverage",
    "view-coverage": "open ./coverage/lcov-report/index.html"
  },
  "author": "Ken Snyder <kendsnyder@gmail.com>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@vitest/coverage-istanbul": "2.0.2",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "typescript": "5.5.3",
    "vitest": "2.0.2"
  }
}
